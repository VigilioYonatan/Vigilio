{
    "name": "@vigilio/react-icons",
    "version": "0.0.2",
    "description": "Librería de iconos con importación modular react - preact",
    "sideEffects": false,
    "peerDependencies": {
        "react": ">=10"
    },
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./fontawesome": {
            "import": {
                "types": "./dist/fontawesome.d.ts",
                "default": "./dist/fontawesome.js"
            },
            "require": {
                "types": "./dist/fontawesome.d.cts",
                "default": "./dist/fontawesome.cjs"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/VigilioYonatan/Vigilio/blob/main/react-fetching/Readme.md"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "react",
        "preact",
        "icons"
    ],
    "scripts": {
        "build:esm": "tsc",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "build": "bunx tsup"
    },
    "author": "Vigilio",
    "license": "ISC",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-swc": "^0.4.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.4",
        "@rsbuild/core": "^1.4.8",
        "@rsbuild/plugin-react": "^1.3.4",
        "@rspack/cli": "^1.4.8",
        "@types/react": "^18.2.14",
        "esbuild": "^0.25.8",
        "react": "^18.2.0",
        "rollup": "^4.45.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-visualizer": "^6.0.3",
        "tsdown": "^0.12.9",
        "tslib": "^2.6.0",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@swc/core": "^1.13.1"
    }
}
